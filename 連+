Private Sub Button1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button1.Click
        s(Val(TextBox1.Text))
    End Sub
    Sub s(ByVal a As Integer)
        Dim w As Integer
        For i = 1 To a
            w = w + i
        Next
        TextBox2.Text = w
    End Sub
End Class
